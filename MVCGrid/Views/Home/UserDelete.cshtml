@using MVCGrid.Models
@model Register

@{
    ViewBag.Title = "UserDelete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserDelete</h2>

@using (Html.BeginForm("DeleteUser","home","prasad"))
{
    <table>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Username</th>
        </tr>
        <tr>
            <td>@Model.Name</td>
            <td>@Model.Email</td>
            <td>@Model.Mobile</td>
            <td>@Model.Username</td>
        </tr>
    </table>
    @Html.ActionLink("Delete", "DeleteUser", new { @id = Model.Username }, null);
}
<div>
    <a href="~/Home/StudentIndex">Back....</a>
</div>
